{
  "name": "eslint-plugin-small-import",
  "version": "1.0.0",
  "description": "ESLint rule for preventing the full import of big libraries when just a part of it can be imported.",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tsc && ava dist/test/*.js",
    "prepublish": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JonnyBurger/eslint-plugin-react-native-normalized.git"
  },
  "keywords": [
    "eslint",
    "plugin",
    "lodash",
    "import",
    "small",
    "bundle-size"
  ],
  "author": "Jonny Burger <jonny.io>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JonnyBurger/eslint-plugin-react-native-normalized/issues"
  },
  "homepage": "https://github.com/JonnyBurger/eslint-plugin-react-native-normalized#readme",
  "peerDependencies": {
    "eslint": "^6.1.0"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "eslint-ava-rule-tester": "^4.0.0",
    "typescript": "^3.7.3",
    "@jonny/eslint-config": "^2.1.37"
  },
  "dependencies": {
    "eslint": "^6.7.2"
  }
}
